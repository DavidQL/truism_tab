{
  "name": "Truism tab",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "See a Jenny Holzer Truism quote every time you open a new tab.",

  "icons": {
     "128": "icons/truism_tab.png"
  },

  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "http://*/*",
    "tabs"
  ]
}